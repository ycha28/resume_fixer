<!-- Submit Section -->
<section id="submit" class="bg-light-gray">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading">Submit Documents</h2>
            </div>
            <div class="col-md-6 col-centered">
                <h3 class="section-subheading text-muted" align="justify"><br><br>Submit your <strong>resumes</strong>, <strong>cover letters</strong>, and <strong>application essays</strong> for review.<br><br>You will receive edits <strong>within 24 hours of submission</strong>.<br><br>To attach multiple files, make sure that all files of the same type are already in the same folder. Click "Browse" and locate that folder. Then select all files and press "Open." You should see multiple file icons appear.<br><br>Make sure to add detailed comments about your files in the comment boxes so we understand your specific needs!<br><br>Once you have selected your documents, click the "Submit & Pay" button. You will receive a confirmation email. <br><br>Acceptable file formats include: <strong>.pdf</strong>, <strong>.docx</strong>, <strong>.doc</strong> and <strong>.txt.</strong></h3>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6 col-md-offset-3"> 
              <%= form_for @submission, multipart: true do |f| %>
                <div class="row">
                  <div id="cover_letters" class="form-group">
                    <%= f.label :cover_letters, "Cover Letters" %>
                    <%= f.file_field :cover_letters, multiple: true, type: :file, class: 'form-control submission_file cover_letter' %>
                  </div>
                  <div id="resumes" class="form-group">
                    <%= f.label :resumes %>
                    <%= f.file_field :resumes, multiple: true, type: :file, class: 'form-control submission_file resume' %>
                  </div>
                  <div id="essays" class="form-group">
                    <%= f.label :essays %>
                    <%= f.file_field :essays, multiple: true, type: :file, class: 'form-control submission_file essay' %>
                  </div>
                </div>

                <div class="row total-price">
                  <h4>Total Price: <span id="total_price">0</span></h4>
                </div>  

                <div class='row text-center'>
                  <script src="https://checkout.stripe.com/checkout.js" class="stripe-button"
                          data-key="<%= Rails.configuration.stripe[:publishable_key] %>"
                          data-description="Submit Documents"
                          data-label="Submit &amp Pay"></script>
                </div>
              <% end %>
            </div>
        </div>
    </div>
</section>

